                                    transform .2s;
                                } */
    .parent-div {
      /* width: 320px; */
      /* height: 390px; */
      overflow: hidden;
      position: relative;
    }

    /* slider */

    .slider {
      position: relative;
      /* width: 100%; */
      /* width: 1000px; */
      overflow: hidden;
      margin: auto;
    }

    /* Slides container */
    .slides {
      display: flex;
      transition: transform 0.5s ease-in-out;
    }

    /* Each slide */
    @media only screen and (max-width: 767px) {
      .slide {
        min-width: 100%;
        margin-right: 0px;
        border-radius: 5px;
      }
    }

    @media screen and (min-width: 768px) and (max-width: 1199px) {
      .slide {
        min-width: 50%;
        margin-right: 0px;
        border-radius: 5px;
      }
    }

    @media screen and (min-width: 1200px) {
      .slide {
        min-width: 33.3%;
        margin-right: 20px;
        border-radius: 5px;
      }
    }

    /* Navigation buttons */
    .prev,
    .next {
      position: absolute;
      top: 50%;
      transform: translateY(-50%);
      background-color: rgba(0, 0, 0, 0.5);
      color: white;
      border: none;
      padding: 10px;
      cursor: pointer;
    }

    .prev {
      left: 0;
    }

    .next {
      right: 0;
    }

    .animate-wheel {
      animation: spin 4s linear infinite;
    }

    .animate-item-triangle {
      position: absolute;
      animation-name: triangle;
      animation-duration: 6s;
      animation-iteration-count: infinite;
    }
    .animate-item-ylevel {
      position: absolute;
      animation-name: ylevel;
      animation-duration: 5s;
      animation-iteration-count: infinite;
    }
    .animate-item-xlevel {
      position: absolute;
      animation-name: xlevel;
      animation-duration: 5s;
      animation-iteration-count: infinite;
    }

    @keyframes spin {
      0% {
        transform: rotate(0deg);
        color: orange;
      }
      100% {
        transform: rotate(360deg);
        color: rgb(8, 77, 180);
      }
    }

    @keyframes triangle {
      0% {
        color: red;
        /* left: 20px;
            top: 20px; */
        transform: translateY(-30px);
      }
      25% {
        color: orange;
        /* left: 40px;
            top: 20px; */
        transform: translateX(-10px);
      }
      50% {
        color: blue;
        /* left: 40px;
            top: 40px; */
        transform: translateY(-30px);
      }
      75% {
        color: rgb(15, 214, 15);
        /* left: 20px;
            top: 40px; */
        transform: translateX(10px);
      }
      100% {
        color: red;
        /* left: 20px;
            top: 20px; */
        transform: translateY(-30px);
      }
    }
    @keyframes ylevel {
