  <script>
    let currentIndex = 0;

    function showSlide(index) {
      const slides = document.querySelector('.slides');
      let totalSlides = document.querySelectorAll('.slide').length / 2;
      const screenWidth = window.innerWidth;

      if (screenWidth < 768) {
        totalSlides = document.querySelectorAll('.slide').length;
      } else if (screenWidth >= 768 && screenWidth < 1200) {
        totalSlides = document.querySelectorAll('.slide').length / 2 + 1;
      }

      if (index >= totalSlides) {
        currentIndex = 0; // Loop back to the first slide
      } else if (index < 0) {
        currentIndex = totalSlides - 1; // Loop back to the last slide
      } else {
        currentIndex = index;
      }

      if (screenWidth < 768) {
        slides.style.transform = `translateX(-${currentIndex * 100}%)`;
      } else if (screenWidth >= 768 && screenWidth < 1200) {
        slides.style.transform = `translateX(-${currentIndex * 50}%)`;
      } else {
        slides.style.transform = `translateX(-${currentIndex * 36}%)`;
      }
    }

    function nextSlide() {
      showSlide(currentIndex + 1);
    }

    // function prevSlide() {
    //   showSlide(currentIndex - 1);
    // }

    setInterval(() => {
      nextSlide();
    }, 2000);
  </script>

